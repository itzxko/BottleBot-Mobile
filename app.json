{
  "expo": {
    "name": "Bottle-Bot",
    "slug": "Bottle-Bot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.itzxko.BottleBot"
    },
    "notification": {
      "icon": "./assets/images/Bottle_Bot.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Bottle_Bot.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.itzxko.BottleBot",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE",
        "INTERNET"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB2dl6JPoqsy-H_PjfzF6lnom63z_3E1J0"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow ${PRODUCT_NAME} to access your camera",
          "microphonePermission": "Allow ${PRODUCT_NAME} to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fad98cb7-126b-4133-b3fc-066c43c5cb37"
      }
    },
    "owner": "itzxko0"
  }
}
